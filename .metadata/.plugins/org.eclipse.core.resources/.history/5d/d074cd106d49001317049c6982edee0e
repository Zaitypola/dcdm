class Location:
    def __init__(self,name,address,city,country,age,theme,photo_url):
        self.name=name
        self.address=address
        self.city=city
        self.country=country
        self.age=age
        self.theme=theme
        self.photo_url=photo_url       
    def toString(self):
        print "Name: " + self.name + " - Address: " +self.address + " - City: " + self.city + " - Country: " + self.country + " - Age: " + str(self.age) + " - Theme: " + self.theme + " - Photo: " + self.photo_url 